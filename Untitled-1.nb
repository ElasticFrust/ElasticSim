Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["d", "e"], " ", "=", " ", 
    SuperscriptBox["V", "\[Mu]"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a2a12ee7-4069-e04c-9d85-1df5e7a18913"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", 
      SuperscriptBox["e", "T"], ")"}], "\[Mu]"], " "}], TraditionalForm]],
  ExpressionUUID->"215dbf34-38be-b64d-afd7-e1abd4b77423"],
 "  where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["V", "\[Mu]"], TraditionalForm]],ExpressionUUID->
  "47f9b600-875b-cd49-b0ea-cd87b83b5a8e"],
 " is the vector between centroid and any point on the edge e.  \n",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"(", 
     SuperscriptBox["e", "T"], ")"}], "\[Mu]"], TraditionalForm]],
  ExpressionUUID->"cc9d7adb-2be4-6147-8819-588bd6c49285"],
 "= ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["g", "\[Mu]\[Nu]"], 
     SuperscriptBox[
      SuperscriptBox["e", "*"], "\[Nu]"]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{
        SubscriptBox["l", "e"], 
        SqrtBox["g"]}]], 
      SubscriptBox["g", "\[Mu]\[Nu]"], 
      SuperscriptBox["\[Epsilon]", "\[Nu]\[Rho]"], 
      SubscriptBox["g", "\[Rho]\[Sigma]"], 
      SuperscriptBox["e", "\[Sigma]"]}], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", 
        RowBox[{
         SubscriptBox["l", "e"], 
         SqrtBox["g"]}]], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SuperscriptBox[
           SubscriptBox["\[Delta]", "1"], "\[Nu]"], 
          SuperscriptBox[
           SubscriptBox["\[Delta]", "2"], "\[Rho]"]}], "-", 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["\[Delta]", "1"], "\[Rho]"], 
          SuperscriptBox[
           SubscriptBox["\[Delta]", "2"], "\[Nu]"]}]}], ")"}], 
       SubscriptBox["g", "\[Mu]\[Nu]"], 
       SubscriptBox["g", "\[Rho]\[Sigma]"], 
       SuperscriptBox["e", "\[Sigma]"]}], " ", "=", 
      RowBox[{
       FractionBox["1", 
        RowBox[{
         SubscriptBox["l", "e"], 
         SqrtBox["g"]}]], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["g", "\[Mu]1"], 
          SubscriptBox["g", 
           RowBox[{"2", "\[Sigma]"}]]}], "-", 
         RowBox[{
          SubscriptBox["g", "\[Mu]2"], 
          SubscriptBox["g", 
           RowBox[{"1", "\[Sigma]"}]]}]}], ")"}], 
       SuperscriptBox["e", "\[Sigma]"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a821dbcf-f7d9-5b41-a1b1-360030f1bfa0"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["d", "e"], "=", 
    RowBox[{
     SubscriptBox[
      RowBox[{
       SuperscriptBox["V", "\[Mu]"], "(", 
       SuperscriptBox["e", "T"], ")"}], "\[Mu]"], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", 
        RowBox[{
         SubscriptBox["l", "e"], 
         SqrtBox["g"]}]], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["g", "\[Mu]1"], 
          SubscriptBox["g", 
           RowBox[{"2", "\[Sigma]"}]]}], "-", 
         RowBox[{
          SubscriptBox["g", "\[Mu]2"], 
          SubscriptBox["g", 
           RowBox[{"1", "\[Sigma]"}]]}]}], ")"}], 
       SuperscriptBox["V", "\[Mu]"], 
       SuperscriptBox["e", "\[Sigma]"]}], " ", "=", 
      RowBox[{
       FractionBox["1", 
        RowBox[{
         SubscriptBox["l", "e"], 
         SqrtBox["g"]}]], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["g", "\[Mu]1"], 
          SubscriptBox["g", 
           RowBox[{"2", "\[Sigma]"}]], 
          SuperscriptBox["V", "\[Mu]"], 
          SuperscriptBox["e", "\[Sigma]"]}], "-", 
         RowBox[{
          SubscriptBox["g", "\[Mu]2"], 
          SubscriptBox["g", 
           RowBox[{"1", "\[Sigma]"}]]}]}], ")"}]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7008edaf-db74-114a-bcd5-cd810920cc44"],
