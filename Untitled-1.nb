Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    FractionBox["1", 
     SqrtBox["g"]], 
    SuperscriptBox["\[Epsilon]", "\[Mu]\[Rho]"], 
    SubscriptBox["g", "\[Rho]\[Sigma]"], 
    SuperscriptBox["e", "\[Sigma]"], 
    FractionBox["1", 
     SqrtBox["g"]], 
    SuperscriptBox["\[Epsilon]", "\[Nu]\[Tau]"], 
    SubscriptBox["g", "\[Tau]\[Lambda]"], 
    SuperscriptBox["e", "\[Lambda]"], 
    SubscriptBox["g", "\[Mu]\[Nu]"]}], " ", "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SqrtBox["g"]], 
     SuperscriptBox["\[Epsilon]", "\[Mu]\[Rho]"], 
     SubscriptBox["g", "\[Rho]\[Sigma]"], 
     SuperscriptBox["e", "\[Sigma]"], 
     SqrtBox["g"], 
     SubscriptBox["\[Epsilon]", "\[Mu]\[Lambda]"], 
     SuperscriptBox["e", "\[Lambda]"]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SuperscriptBox["\[Delta]", "\[Rho]"], ")"}], "\[Lambda]"], 
      SubscriptBox["g", "\[Rho]\[Sigma]"], 
      SuperscriptBox["e", "\[Sigma]"], 
      SuperscriptBox["e", "\[Lambda]"]}], "=", 
     RowBox[{
      SubscriptBox["g", "\[Lambda]\[Sigma]"], 
      SuperscriptBox["e", "\[Sigma]"], 
      SuperscriptBox["e", "\[Lambda]"]}]}]}]}], TraditionalForm]], "Input",
