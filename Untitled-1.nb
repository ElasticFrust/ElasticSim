Cell[TextData[{
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", "e"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "60765ab4-dd73-2042-b924-1b5865562cb7"],
 ": Find ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["e", "T"], "->", " ", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        SuperscriptBox["e", "T"], ")"}], "\[Mu]"], 
      SubscriptBox["g", 
       RowBox[{"\[Mu]\[Nu]", " "}]], 
      SuperscriptBox["e", "\[Nu]"]}]}], " ", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"0", " ", "--"}], ">", " ", 
      SuperscriptBox[
       SuperscriptBox["e", "T"], "\[Mu]"]}], "=", " ", 
     RowBox[{
      FractionBox["1", 
       SqrtBox["g"]], 
      SuperscriptBox["\[Epsilon]", "\[Mu]\[Rho]"], 
      SubscriptBox["g", "\[Rho]\[Nu]"], 
      SuperscriptBox["e", "\[Nu]"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b8003b9d-a305-0443-9015-8648917cbd43"],
 ". \nNormalize to get ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", "*"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "af5c9828-a52d-2e45-b1cb-3bb306ddc051"],
 " in regular frame:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SuperscriptBox["e", "*"], "\[Mu]"], "=", " "}], TraditionalForm]],
  InitializationCell->True,
  FormatType->TraditionalForm,ExpressionUUID->
  "84bbda8d-db8c-564a-aa5a-58b977774e70"],
 Cell[BoxData[
  FormBox[
   FractionBox[
    SuperscriptBox[
     SuperscriptBox["e", "T"], "\[Mu]"], 
    SqrtBox[
     RowBox[{
      SubscriptBox["g", "\[Mu]\[Nu]"], 
      SuperscriptBox[
       SuperscriptBox["e", "T"], "\[Mu]"], 
      SuperscriptBox[
       SuperscriptBox["e", "T"], "\[Nu]"]}]]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "329de0b2-b338-e740-9010-96e91e8343e7"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    SuperscriptBox[
     SuperscriptBox["e", "T"], "\[Mu]"], 
    SqrtBox[
     RowBox[{"|", "e", "|", "2"}]]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5eb5ef3d-7565-7947-adb7-faa2a8a41b7e"]
}], "Text",
